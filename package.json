{
  "name": "vishu.io",
  "version": "0.0.1",
  "description": "My blog built with Next.js",
  "main": "app.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "commit": "npx git-cz",
    "check-types": "tsc",
    "lint-staged": "lint-staged",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint": "jest --config test/jest.lint.js",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:validate": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel prettier:validate check-types lint",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:local": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "npm run check-types && jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pvgdev/vishu.io.git"
  },
  "author": "pvgdev <pvgdev@outlook.com> (https://vishu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pvgdev/vishu.io/issues"
  },
  "homepage": "https://github.com/pvgdev/vishu.io#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@types/node": "^14.0.11",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-jest": "^26.0.1",
    "commitizen": "^4.1.2",
    "cypress": "^4.7.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^26.0.1",
    "jest-environment-jsdom": "^26.0.1",
    "jest-environment-node": "^26.0.1",
    "jest-runner-eslint": "^0.9.0",
    "jest-styled-components": "^7.0.2",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "react-testing-library": "^8.0.1",
    "start-server-and-test": "^1.11.0",
    "typescript": "^3.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "image-trace-loader": "^1.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "jimp": "^0.13.0",
    "lqip-loader": "^2.2.0",
    "next": "^9.4.4",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.1",
    "react": "0.0.0-experimental-33c3af284",
    "react-dom": "0.0.0-experimental-33c3af284",
    "responsive-loader": "^1.2.0",
    "styled-components": "^5.1.1",
    "svg-sprite-loader": "^5.0.0",
    "webp-loader": "^0.6.0"
  }
}
